{
  "compilerOptions": {
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "declaration": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": false,
    "noImplicitThis": false,
    "noUnusedParameters": false,
    "importHelpers": true,
    "noEmitHelpers": true,
    "module": "commonjs",
    "pretty": true,
    "jsx": "react",
    "strictNullChecks": false,
    "moduleResolution": "node",
    "target": "es6",
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  "formatCodeOptions": {
    "indentSize": 2,
    "tabSize": 2
  },
  "include": [
    "src/**/*"
  ]
}